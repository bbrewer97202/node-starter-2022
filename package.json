{
  "name": "node-starter-2022",
  "version": "1.0.0",
  "description": "Node starter 2022",
  "type": "module",
  "exports": "./dist/index.js",
  "scripts": {
    "build": "npm run clean && swc ./src/* -d ./dist",
    "build:dev": "npm run build && NODE_ENV=development node --inspect dist/index.js",
    "clean": "rimraf dist",
    "dev": "nodemon --ext ts --exec \"npm run build:dev\"",
    "start": "npm run build && NODE_ENV=production node ./dist/index.js",
    "lint:old": "eslint src --ext .ts",
    "lint": "eslint 'src/**/*' --max-warnings=0",
    "prepare": "husky install",
    "test": "echo \"no test specified\" && exit 0"
  },
  "keywords": [],
  "author": "bbrewer97202@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.3",
    "@tsconfig/node16": "^1.0.3",
    "@types/express": "^4.17.14",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.20",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "dotenv": "^16.0.2",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "express": "^4.18.1",
    "morgan": "^1.10.0"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint"
  }
}
